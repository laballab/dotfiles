" .vimrc settings

" general
let base16colorspace=256
set t_Co=256
syntax on
let mapleader=";"

" line num 
set relativenumber
set number

" tab
set tabstop=2
set softtabstop=2
set shiftwidth=2
set shiftround
set expandtab
set smarttab

" indent
set autoindent
set smartindent

" search
set ignorecase
set smartcase
