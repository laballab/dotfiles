{
    "editor.fontSize": 16,
    "window.zoomLevel": 0,
    "editor.stopRenderingLineAfter" : -1,
    "editor.minimap.enabled": false,
    "editor.suggest.statusBar.visible": true,
    "explorer.confirmDelete": false,
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/ballablx/.config/nvim/init.vim",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "terminal.integrated.fontSize": 14,
    "git.untrackedChanges": "separate",
    "json.maxItemsComputed": 6000,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "explorer.autoReveal": false,
    "explorer.openEditors.visible": 0,
    "workbench.colorTheme": "dark-plus-syntax",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.project.importHint": false,
    "java.configuration.checkProjectSettingsExclusions": false,
    "workbench.settings.openDefaultKeybindings": true,
    "python.languageServer": "Microsoft",
    "python.linting.flake8CategorySeverity.W": "Error",
    "vim.useSystemClipboard": true,
    "vim.sneak": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "explorer.confirmDragAndDrop": false,
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "extensions.ignoreRecommendations": true,
    "yaml.validate": false,
    "go.useLanguageServer": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.fastScrollSensitivity": 3,
    "terminal.integrated.scrollback": 5000,
    "vim.smartRelativeLine": true,
    "vim.sneakReplacesF": true
}
