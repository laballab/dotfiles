! ------------------------------------------------------------------------------
! ROFI 
! ------------------------------------------------------------------------------

rofi.color-enabled: 	true
rofi.color-normal: 	base00, base04, base00, base04, base00 
rofi.color-urgent: 	#273238, #ff1844, #273238, #394249, #ff1844
rofi.color-active: 	#273238, #80cbc4, #273238, #394249, #80cbc4
rofi.color-window: 	base00, base04, base04
rofi.separator-style:   solid

! ------------------------------------------------------------------------------
! FONTS 
! ------------------------------------------------------------------------------

Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.dpi: 96
Xft.rbga: rgb
Xft.lcdfilter: lcddefault

! ------------------------------------------------------------------------------
! URXVT
! ------------------------------------------------------------------------------

xvt*geometry:                115x40
URxvt*font: xft:Terminus:size=10:antialias=true:hinting=true
URxvt*boldFont: xft:Terminus:bold:size=10:antialias=true:hinting=true
URxvt*depth:      32
URxvt*borderless: 1
URxvt*scrollBar:  false
URxvt*saveLines:  2000
URxvt.transparent:false 
URxvt*.shading:   30





