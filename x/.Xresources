! ------------------------------------------------------------------------------
! FONTS 
! ------------------------------------------------------------------------------

Xft.autohint:   0
Xft.antialias:  1
Xft.hinting:    true
Xft.hintstyle:  hintfull
Xft.dpi:        96
Xft.rbga:       rgb
Xft.lcdfilter:  lcddefault

! ------------------------------------------------------------------------------
! URXVT
! ------------------------------------------------------------------------------

xvt*geometry:     115x40
URxvt*font:       xft:Terminus:size=10:antialias=true:hinting=true
URxvt*boldFont:   xft:Terminus:bold:size=10:antialias=true:hinting=true
URxvt*depth:      32
URxvt*borderless: 1
URxvt*scrollBar:  false
URxvt*saveLines:  2000
URxvt*transparent:false 
URxvt*shading:    30





